<%- include("header") -%>
  <div class="box" id="heading">
    <h1>hello this is <%=kindOfDay%> </h1>
  </div>
  <div class="box">
    <% for(var i =0;i<newItem.length ;i++){ %>
    <div class="item">
      <input type="checkbox">
      <p> <%=newItem[i]%> </p>
    </div>
    <%}%>
      <form class="item" action="/" method="post">
        <input type="text" name="new" autocomplete="off" placeholder="enter the new item">
        <button type="submit" name="button" value=<%=kindOfDay%>>+</button>
      </form>
    </div>

<%- include("footer") -%>
